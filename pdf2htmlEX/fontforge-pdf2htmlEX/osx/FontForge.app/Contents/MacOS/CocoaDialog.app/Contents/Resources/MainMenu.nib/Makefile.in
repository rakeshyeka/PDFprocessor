install-ffappDATA: $(ffapp_DATA)
	@$(NORMAL_INSTALL)
	@list='$(ffapp_DATA)'; test -n "$(ffappdir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(ffappdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(ffappdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(ffappdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(ffappdir)" || exit $$?; \
	done

uninstall-ffappDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(ffapp_DATA)'; test -n "$(ffappdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(ffappdir)'; $(am__uninstall_files_from_dir)
